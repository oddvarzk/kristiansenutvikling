{
  "redirects": [
    {
      "source": "/:path*/index:(html|php)",
      "destination": "/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.kristiansenutvikling.no" }],
      "destination": "https://kristiansenutvikling.no/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "kristiansenutvikling.com" }],
      "destination": "https://kristiansenutvikling.no/:path*",
      "permanent": true
    }
  ]
}
